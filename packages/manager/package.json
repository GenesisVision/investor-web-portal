{
  "name": "manager-web-portal",
  "homepage": "",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "shared": "*",
    "gv-api-web": "*"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "yarn copy && react-app-rewired start",
